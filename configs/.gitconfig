[user]
  name = Yitzhak B. Solorzano
  email = betancour@gmail.com 
  username = betancour
[core]
  editor = vim
  whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol
  excludefiles = ~/.gitignore
[color]
  ui = auto
[diff]
  tool = vimdiff
[difftool]
  prompt = false
[color "branch"]
  current = yellow bold
  local = green bold
  remote = cyan bold
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
  whitespace = red reverse
[color "status"]
  added = green bold
  changed = yellow bold
  untracked = red bold
[gitflow "prefix"]
  versiontag = v
